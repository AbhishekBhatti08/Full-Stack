/* ------------------------------------------Side Panel Styling --------------------------------------------*/
           .sidepanel  {
            display: block;
            width: 0;
            position: fixed;
            z-index: 1;
            height: 400px;
            top: 60px;
            left: 0;
            background-color: white;
            overflow-x: hidden;
            transition: 0.5s;
            padding-top: 60px;
            border-bottom:1px solid thistle;
            border-right: 1px solid thistle;
            }

            .sidepanel a {
            padding: 8px 8px 8px 32px;
            text-decoration: none;
            font-size: 20px;
            color: #1DA1F2;
            display: block;
            transition: 0.3s;
            }

            /* .sidepanel a:hover {
            color: #f1f1f1;
            } */

            .anchor:hover {
                color: black;
            }

            .sidepanel .closebtn {
            position: absolute;
            top: 0;
            right: 25px;
            font-size: 36px;
            }

            .openbtn {
            display: block;
            font-size: 20px;
            cursor: pointer;
            background-color: white;
            color: #1DA1F2;;
            padding: 10px 15px;
            border: none;
            }

            .openbtn:hover {
            background-color:#444;
            }
            




