<body>

    <style>
        * {
            box-sizing: border-box;
        }

        body {

            display: flex;

        }

        /* ----------------------- Left Section Styling   ---------------------------------------------------------*/



        .left-section {
            display: block;
            width: 300px;

        }

        /* nav {
            display: none;
        } */
        
        .sidepanel {
            display: none;
        }

        .openbtn {
            display: none;
        }

        .twitterLogo {
            border-radius: 50%;
            margin-left: 35px;
            margin-top: 10px;
            margin-bottom: 20px;

        }

        .imgLogo {
            height: 3em;
            width: 3em;
        }

        .explore,
        .homeLogo,
        .notifications,
        .messages,
        .profile,
        .more,
        .btn-primary {
            display: flex;
            margin-bottom: 20px;
            margin-left: 20px;
            align-items: center;
            /* justify-content:left; */
            /* align-content: flex-start; */
            border-radius: 15px;
            width: 200px;
            height: 40px;
            /* background-color: coral; */
        }

        .explore:hover,
        .homeLogo:hover,
        .notifications:hover,
        .messages:hover,
        .profile:hover,
        .more:hover,
        .primaryButton:hover,
        .bi-three-dots:hover {

            background-color: #d8ecf3;
            color: #1DA1F2;
            cursor: pointer;
            /* border-color: #1DA1F2; */

        }

        /* {
            border-color: #1DA1F2;
        } */

        h3,
        .bi {
            display: inline-block;
            margin-left: 20px;
        }

        h3 {
            font-size: 20px;
        }

        .bi-three-dots {
            border-radius: 50%;
            /* border: 2px solid black; */

        }

        .primaryButton {
            border-radius: 15px;
            width: 235px;
            font-weight: bolder;
            align-content: center;
            border: 0px;
        }

        .btn {
            border: 0px;
        }

        .tweet {
            margin: 5px auto;
            font-size: 20px;

        }


        /* ------------------------------Main Section Styling------------------------------------------------------- */

       


        .header {
            display: flex;
            align-items: center;
            justify-content: space-between;
            border: 2px solid #d3d3d3;
            border-top: 0px;
            width: 600px;
            height: 60px;
            padding: 10px;
            font-weight: bolder;

        }

        span {
            display: none;
        }

        .person-icon {
            display: none;
        }

        .writeTweet {
            border: 2px solid #d3d3d3;
            border-top: 0px;
            border-bottom: 0px;
            padding-right: 15px;

        }

        .bi-signpost {
            color: #1DA1F2;
        }


        .tweetLine1 {
            display: flex;
            align-items: center;
            Height: 80px;
        }

        .placeholder {
            padding: 10px;
            margin: 0px;
            font-size: larger;
            color: grey;

        }

        .bi-person-circle {
            padding-top: 10px;
            margin-left: 8px;
            color: #708090;
        }


        .tweetLine2 {
            display: flex;
            align-items: baseline;
            margin-left: 80px;
            /* padding-top: 10px; */
            margin-top: 10px;
            justify-content: space-between;
            /* align-items: center; */
            color: #1DA1F2;

        }

        .tweetButton {
            width: 70px;
            height: 35px;
            justify-content: space-between;

        }

        .status {
            display: flex;
            align-items: center;
            margin-left: 80px;
            border-bottom: 1px solid #d3d3d3;


        }

        .whoCan {
            margin: 0px;
            padding: 10px;
            font-weight: bolder;
            color: #1DA1F2;
        }

        .bi-image,
        .bi-globe2 {
            margin-left: 0px;

        }



        .bi-globe2 {
            color: #1DA1F2;
        }

        .bi-file-arrow-up,
        .bi-file-bar-graph,
        .bi-emoji-smile,
        .bi-calendar2 {
            margin-left: 10px;
        }

        /* ---------------------------------------------Main Tweet Styling --------------------------------------------*/
        .container {

            padding: 0px;
            padding: 0px;
            border: 2px solid #d3d3d3;
            /* width: 500px; */

        }

        .tweets {
            margin: 0px;
            padding: 0px;

            border-top: 0px;
            border-left: 20px;
        }

        .tweet-head {
            display: flex;
            align-items: center;
            justify-content: space-between;

        }

        .left-head {
            display: flex;
            align-items: center;

        }

        .username {
            margin: 0px 10px;
            border: 0px;
            font-size: 20px;
        }

        .dots {

            padding: 10px;

        }

        .tweet-body {
            margin-left: 80px;
            height: 100px;
            /* background-color: honeydew; */
            /* border: 1px solid rgb(180, 177, 177); */
            margin-right: 20px;
            border-radius: 15px;
        }

        .tweet-footer {
            margin-left: 80px;
            display: flex;
            align-items: center;



        }

        .footer-icons {
            width: 400px;
            display: flex;
            justify-content: space-between;

        }


        .bi-arrow-bar-up,
        .bi-heart,
        .bi-arrow-repeat,
        .bi-chat {
            margin: 0px;
        }

        .bi-arrow-bar-up:hover,
        .bi-arrow-repeat:hover,
        .bi-chat:hover {

            color: #1DA1F2;
            border-radius: 50%;
            cursor: pointer;
        }

        .bi-heart:hover {
            color: #ff4c4c;
            cursor: pointer;
        }


        /* ---------------------------------- Right Column Styling-------------------------------------------------- */

        .right-column {
            /* background-color: hotpink; */
            width: 100%;
            /* display: flex; */


        }

        .search {
            display: flex;
            margin: 30px 60px;
            align-items: center;
            justify-content: flex-start;
            border: 1px solid #d3d3d3;
            height: 45px;
            border-radius: 25px;
            color: grey;
            background-color: #E8F6FE;
        }

        .search:hover {
            cursor: pointer;
            background-color: #BBE3FB;
        }

        .search-content {
            margin-bottom: 0%;
            margin-left: 20px;
        }

        .updates {
            background-color: #E8F6FE;
            margin: 0px 20px;
            border-top-left-radius: 15px;
            border-top-right-radius: 15px;
            height: 550px;
            border-radius: 15px;

        }

        .updates-header {
            padding: 10px;
            font-weight: bolder;
            font-size: larger;

        }

        .updates-header:hover {
            cursor: pointer;
            background-color: #BBE3FB;
            
        }

        .update-1 {
            display: flex;
            justify-content: space-between;

        }

        .update-1:hover{
            background-color: #BBE3FB;
            cursor: pointer;
        }


        .ipl-image {
            height: 90px;
            width: 110px;
            border-radius: 15px;
            padding: 10px;
        }

        .above-hash {
            display: flex;
            padding-left: 10px;
            font-size: smaller;
        }

        .news-1 {
            padding-left: 10px;
        }

        .sub-newsheading {
            font-size: smaller;
        }

        .news-content {
            font-weight: bold;
        }

        .follow-content{
            display: flex;

        }

        .follow-section {
            /* background-color: #e5e5ff; */
            background-color: #E8F6FE;
            margin: 40px 20px;
            border-top-left-radius: 15px;
            border-top-right-radius: 15px;
            height: 530px;
            border-radius: 15px;
            /* display: flex; */

        }


        .follow-header {
            margin: 0%;
            /* background-color: coral; */
        }

        .follow-content1{
            display: flex;
            align-items: unset;
            margin-top: 15px;
        }


        .img-follow {
            border-radius: 50%;
            width: 5em;
            height: 5em;
            padding: 10px;
        }

        .follow-icon{
            height: 1em;
            width: 1em;
        }

        .follow-button {
            border: 1px solid #1DA1F2;
            border-radius: 25px;
            /* background-color: coral; */
            padding: 0%;
            color: #1DA1F2;
            font-weight: bolder;  
            width: 100px;
            height: 30px;
            text-align: center;
            margin-left: 10px;          
        }

        .follow-button:hover {
            cursor: pointer;
            background-color:  #1DA1F2;
            color: whitesmoke;
        }

        .above-hash:hover {
        cursor: pointer;
        background-color: #BBE3FB;
        }

        .follow-content:hover{
            cursor: pointer;
        background-color: #BBE3FB;

        }


